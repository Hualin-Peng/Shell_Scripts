# include <stdio.h>
# include <math.h>

int count = 0;
//enum BOOL {false=0, true=1};
enum BOOL {false, true};

enum BOOL check(int record[], int i, int j){ //ith row, jth column
    int k = 0;
    while(k<i){
        if(record[k]==j || fabs(j-record[k])==fabs(i-k))
	    return false;
	k++;
    }
    return true;
}

void placeQueen(int record[], int k, int n){
    if(k>n){
        for(int i=0; i<n; i++){
	    printf("%d ", record[i]);
	    //record[i]=0;
	}
	printf("\n");
	count++;
    }else{
	for(int i=1; i<=n; i++){
	    if(check(record, k-1, i)==1){
		record[k-1] = i;
	        placeQueen(record, k+1, n);
	    }
	    else
		;
	}
    }
}

void NQueen(int record[], int n){
    placeQueen(record, 1, n);
    printf("%d\n", count);
}

int main(){
    int record[8] = {0};
    NQueen(record, 8);

    return 0;
}
